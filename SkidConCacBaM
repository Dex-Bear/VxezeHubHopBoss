
local player = game.Players. LocalPlayer


local banDuration = Chờ Đơi Là Hạnh Phúc
local reason = " Api Down Nha ae để tụi tui cố Gắng mở lại | mọi người có thể vào sv discord và xài script khác bên mik nhé" 

local message = string.format("You were kicked from this experience: \nYou are banned for [%d hours]\nReason: %s\nFuture bans will be PERMANENT. Do not break the rules.",
banDuration,
reason
)
player: Kick(message)
